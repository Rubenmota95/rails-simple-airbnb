<div class="container d-flex justify-content-between p-3 mb-2 bg-light text-dark">
  <h1>All our Flats(<%= @flats.count %>)</h1>
</div>
<div>
  <div id="flats">
    <% @flats.each do |flat| %>
      <%= render flat %>
      <p>
        <%= link_to "More Info", flat, class: "btn btn-primary" %>
      </p>
    <% end %>
  </div>
</div>
<br>
